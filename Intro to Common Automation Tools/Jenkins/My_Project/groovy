node {
    stage('get git') {
            git 'https://github.com/gagareg/jenkins.git'
        }
    

    dir('/var/lib/jenkins/workspace/Mvn_Pipeline/mvn/my-app') {
       
        stage('clean and package') {
            sh 'mvn clean package'
        }
        
        stage('test') {
            sh 'mvn test'
        }
        
        stage('archive') {
            archiveArtifacts 'target/*.jar'
        }
        
    }
}

node('Slave_1_18.222.151.197') {
    sh 'ls'    
}



